{
  "name": "react-error-decoder",
  "version": "2.1.6",
  "description": "⚛︎ Decode React minified error message",
  "keywords": [
    "react",
    "error",
    "decoder",
    "react-error-decoder",
    "debug",
    "⚛︎"
  ],
  "author": "omrilotan",
  "license": "Unlicense",
  "homepage": "https://omrilotan.com/react-error-decoder/",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/omrilotan/react-error-decoder.git"
  },
  "files": [
    "index.js",
    "index.mjs",
    "index.d.ts"
  ],
  "type": "commonjs",
  "main": "./index.js",
  "module": "./index.mjs",
  "browser": "./index.mjs",
  "exports": {
    "./package.json": "./package.json",
    "./index.d.ts": "./index.d.ts",
    ".": {
      "browser": {
        "import": "./index.mjs",
        "require": "./index.js"
      },
      "node": {
        "import": "./index.mjs",
        "require": "./index.js"
      },
      "import": "./index.mjs",
      "require": "./index.js",
      "default": "./index.js"
    }
  },
  "types": "index.d.ts",
  "scripts": {
    "prepare": "curl https://raw.githubusercontent.com/facebook/react/main/scripts/error-codes/codes.json > src/collection.json",
    "build": "./scripts/build.sh",
    "format": "prettier --write .",
    "test": "./scripts/test.sh",
    "prepublishOnly": "npm run build"
  },
  "devDependencies": {
    "esbuild": "^0.24.2",
    "prettier": "^3.0.3",
    "typescript": "^5.2.2"
  }
}
