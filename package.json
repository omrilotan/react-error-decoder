{
  "name": "react-error-decoder",
  "version": "2.1.12",
  "description": "⚛︎ Decode React minified error message",
  "keywords": [
    "react",
    "error",
    "decoder",
    "react-error-decoder",
    "debug",
    "⚛︎"
  ],
  "author": "omrilotan",
  "license": "Unlicense",
  "homepage": "https://omrilotan.com/react-error-decoder/",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/omrilotan/react-error-decoder.git"
  },
  "files": [
    "index.js",
    "index.mjs",
    "index.d.ts"
  ],
  "type": "commonjs",
  "main": "./index.js",
  "module": "./index.mjs",
  "browser": "./index.mjs",
  "exports": {
    "./package.json": "./package.json",
    "./index.d.ts": "./index.d.ts",
    ".": {
      "browser": {
        "import": "./index.mjs",
        "require": "./index.js"
      },
      "node": {
        "import": "./index.mjs",
        "require": "./index.js"
      },
      "import": "./index.mjs",
      "require": "./index.js",
      "default": "./index.js"
    }
  },
  "types": "index.d.ts",
  "scripts": {
    "prepare": "curl https://raw.githubusercontent.com/facebook/react/main/scripts/error-codes/codes.json > src/collection.json",
    "build": "tsup src/index.ts --format cjs,esm --dts --out-dir .",
    "format": "prettier --write .",
    "pretest": "echo \"Run tests. To update snapshots run: npm t -- --test-update-snapshots\"",
    "test": "NODE_NO_WARNINGS=1 node --test --experimental-test-coverage --experimental-test-snapshots --experimental-strip-types $@ **/test.ts",
    "prepublishOnly": "node --run build"
  },
  "devDependencies": {
    "@types/node": "^25.3.0",
    "prettier": "^3.0.3",
    "tsup": "^8.5.1",
    "typescript": "^5.2.2"
  }
}
